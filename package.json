{
  "name": "hof-behaviour-loop",
  "version": "1.0.0",
  "description": "A hof plugin allowing for 'loop' behaviour. e.g. 'add another child'",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint && npm run unit && npm run cover && npm run check-coverage && npm audit --audit-level=high",
    "unit": "mocha --recursive test/spec/ --require test/helper --timeout 300000",
    "unit:debug": "mocha --recursive test/spec/ --require test/helper --timeout 300000 --inspect-brk",
    "cover": "nyc mocha --recursive test/spec/ --require test/helper --timeout 300000",
    "check-coverage": "nyc check-coverage --statement 90 --branch 90 --function 90 --line 90",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/UKHomeOfficeForms/hof-behaviour-loop.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UKHomeOfficeForms/hof-behaviour-loop/issues"
  },
  "homepage": "https://github.com/UKHomeOfficeForms/hof-behaviour-loop",
  "dependencies": {
    "hof-behaviour-summary-page": "^3.2.1",
    "hof-theme-govuk": "^4.4.1",
    "lodash": "^4.17.15",
    "mixwith": "^0.1.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.0.0",
    "eslint-config-homeoffice": "^2.2.0",
    "hof-form-controller": "^6.1.0",
    "hof-model": "^3.1.2",
    "mocha": "^7.1.2",
    "nyc": "^15.0.1",
    "reqres": "^3.0.1",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0"
  }
}
